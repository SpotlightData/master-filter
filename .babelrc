{
  "presets": [
    "stage-2",
    "react",
    "es2016"
  ],
  "plugins": [
    "transform-class-properties",
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins": ["dynamic-import-node", "transform-class-properties"]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ],
      "presets": [
        "minify",
        "stage-2",
        "react",
        "es2016"
      ],
    }
  }
}

